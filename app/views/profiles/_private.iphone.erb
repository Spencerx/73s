<h1>Your Dashboard</h1>
<ul selected="true">
<li>  <%= link_to 'View Public Profile', profile_path(@p).add_param(:public_view => true) %> </li> 
<li>  <%= link_to 'Status Updates', "/statuses" %> </li> 
<li>  <%= link_to 'Messages Center (' + @p.received_messages.count.to_s + ')', messages_path unless @p.received_messages.empty? && !@p.has_network? %></li>
<li>  <%= link_to 'Manage Photos', profile_photos_path(@p) %></li>
<li>  <%= link_to 'My Network', profile_friends_path(@p) if @p.has_network? %></li>
<li>  <%= link_to 'The Blog', profile_blogs_path(@p) %></li>
<li>  <%= link_to 'Account Settings', edit_profile_path(@p) %> </li>
</ul>


<%# render :partial => 'profiles/getting_started' %>




