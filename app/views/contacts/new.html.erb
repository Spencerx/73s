<div class="title">
	<h2>New Contact</h2>
<br />
<p>

<%= error_messages_for :contact %>

<% form_for(@contact) do |f| %>
  <p>
    <b>CallSign</b><br />
    <%= f.text_field :callsign %>
  </p>

  <p>
    <b>Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <b>Email</b><br />
    <%= f.text_field :email %>
  </p>

  <p>
    <b>Country</b><br />
    <%= f.text_field :country %>
  </p>

  <p>
    <b>State</b><br />
    <%= f.text_field :state %>
  </p>

  <p>
    <b>City</b><br />
    <%= f.text_field :city %>
  </p>

  <p>
    <b>Frequency</b><br />
    <%= f.text_field :frequency %>
  </p>

  <p>
    <b>Signal Strength</b><br />
    <%= f.text_field :signal %>
  </p>

  <p>
    <b>Event</b><br />
    <%= f.text_field :event %>
  </p>

  <p>
    <b>Notes</b><br />
    <%= f.text_area :notes %>
  </p>

  <p>
    <b>Contacttime</b><br />
    <%= f.datetime_select :contacttime %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>
<br />
<p>
<%= link_to 'Back', contacts_path %>
</p>
<br />

</div>