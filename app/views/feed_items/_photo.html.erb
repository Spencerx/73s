<% p = feed_item.item %>
<div class="feed_item photo_feed" id="<%= feed_item.dom_id %>">
  <div class="left">
	<img src="/images/blurp_photos.jpg" class="png icon_small" title="User Photo Icon"  alt="User Photo Icon" /> 
	<% if p #profile.user  %>
	<%= link_to h(p.profile.f), '/' + p.profile.user.login %> uploaded a <a href="<%= image_path p, nil %>" title="<%= p.caption %>" class="thickbox">new photo</a>
	<% end %>
	</div> 
  	<div class="right">
    	<%= x_feed_link(feed_item) if me %>
  	</div>
	<div class="clear"></div>
</div>