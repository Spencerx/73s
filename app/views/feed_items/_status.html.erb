<%
#locals
status ||= feed_item.item
%>

<% if !status.profile.user.nil? %>

<div class="feed_item photo_feed" id="<%= feed_item.dom_id %>">
  <div class="left">
	<img src="/images/blurp_comment.jpg" class="png icon_small" title="User Photo Icon"  alt="User Photo Icon" /> 
	<% if status.profile.user %>
	<%= link_to h(status.profile.f), '/' + status.profile.user.login %> status <%= time_ago_in_words status.created_at %> ago <%= link_to sanitize(textilize(status.message)), "/" + status.profile.user.login + "/mystatus" %>
	<% end %>
	</div> 
  	<div class="right">
    	<%= x_feed_link(feed_item) if me %>
  	</div>
	<div class="clear"></div>
</div>

<% end %>